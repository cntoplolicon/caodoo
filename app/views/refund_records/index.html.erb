<div class="team_return_goods_top">
  <div class="team_return_goods_top_data">
    <div class="team_return_goods_top_data_number"><span class="green"><%= @total_orders_count %>件</span></div>
    <div class="team_return_goods_top_data_text"><span class="green">总销量</span></div>
  </div>
  <div class="team_return_goods_top_data <%= @returned_orders_count==0 ? 'green' : 'red' %>">
    <div class="team_return_goods_top_data_number"><%= @returned_orders_count %>件</div>
    <div class="team_return_goods_top_data_text">退货量</div>
  </div>
  <div class="team_return_goods_top_data <%= @return_ratio==0 ? 'green' : 'red' %>" style="margin-top: 39px">
    <div class="team_return_goods_top_data_number"><%= number_to_percentage(@return_ratio, precision: 1) %></div>
    <div class="team_return_goods_top_data_text">退货率</div>
  </div>
