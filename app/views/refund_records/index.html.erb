<div class="team_return_goods_bottom">
  <div class="team_return_goods_bottom_title">退货单</div>
  <div class="team_return_goods_bottom_add">
    <%= link_to "添加新退货单", new_contest_team_refund_record_path(@contest_team), class: 'refund_record_new_action' %>
  </div>
  <table>
    <tbody>
    <tr class="team_return_goods_table_title">
      <td class="team_return_order_id_col">订单号</td>
      <td class="team_return_order_count_col">数量</td>
      <td class="team_return_order_receiver_col">收件人</td>
      <td class="team_return_order_express_col">退货物流</td>
      <td class="team_return_order_tracking_number_col">退货运单号</td>
      <td class="team_return_order_status_col">退货状态</td>
      <td class="team_return_order_action_col">操作</td>
    </tr>
    <% @refund_records.each do |r| %>
      <tr>
        <td class="team_return_order_id_col">
          <span class="team_refund_record_order_number"><%= r.order.order_number %></span></td>
        <td class="team_return_order_count_col team_return_goods_count"><%= r.quantity %></td>
        <td class="team_return_order_receiver_col"><%= r.order.receiver %></td>
        <td class="team_return_order_express_col"><%= r.express.name %></td>
        <td class="team_return_order_tracking_number_col">
          <span class="team_refund_record_tracking_number"><%= r.tracking_number %></span></td>
        <% if r.remark.blank? %>
          <td class="team_return_order_status_col">
            <span class="team_refund_record_status_<%= r.status %>"><%= refund_record_status_text(r.status) %></span>
          </td>
        <% else %>
          <td class="team_return_order_status_col">
            <a class="a_live refund_record_remark_action" data-message="<%= r.remark %>"><%= refund_record_status_text(r.status) %>
          </td>
        <% end %>
        <td>
          <% if r.status == RefundRecord::PENDING %>
            <%= link_to '编辑', edit_contest_team_refund_record_path(@contest_team, r), class: 'refund_record_edit_action' %>
          <% else %>
            <span>-</span>
          <% end %>
        </td>
      </tr>
    <% end %>
    </tbody>
  </table>
</div>
<div class="team_return_goods_popup" id="team_return_goods_popup">
</div>
<div id="refund_record_remark_box" class="popup" style="display: none;">
  <div class="resales_popup_box">
    <div class="customer_service_info">
      <ul>
        <li><span style="color: black;">订单备注:</span></li>
        <li class="refund_record_remark_message"></li>
        <li style="text-align: right">
          <a class="close_refund_record_remark_button">知道了</a>
        </li>
      </ul>
    </div>
  </div>
</div>
