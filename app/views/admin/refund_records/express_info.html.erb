<div class="container">
  <dl class="dl-horizontal">
    <dt>快递公司</dt>
    <dd> <%= @refund_record.express.name %></dd>
    <dt>订单号</dt>
    <dd> <%= @refund_record.tracking_number %></dd>
  </dl>
  <% if @express_info[:showapi_res_body][:data].present? && @express_info[:showapi_res_body][:data].any? %>
    <dl class="dl-horizontal">
      <% @express_info[:showapi_res_body][:data].each do |r| %>
        <dt><%= r[:time] %></dt>
        <dd><%= r[:context] %></dd>
      <% end %>
    </dl>
  <% else %>
    <dl class="dl-horizontal">
      <dt>暂无物流信息</dt>
      <dd></dd>
    </dl>
  <% end %>
  <%= link_to "返回", admin_refund_records_path, class: 'btn btn-default' %>
</div>
