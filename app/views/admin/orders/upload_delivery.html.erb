<div class="container form-container">
  <%= form_tag(import_delivery_admin_orders_path, multipart: true) do %>
    <%= label_tag '物流CSV上传：格式（订单号，物流公司，运单号）' %>
    <%= file_field_tag :file, class: 'form-control' %>
        <div style="margin-top: 16px">
      <%= link_to "返回", admin_orders_path, class: 'btn btn-default' %>
      <%= submit_tag '提交', class: 'btn btn-primary' %>
    </div>
  <% end %>
</div>
<div class="container">
  <% if @results %>
    <table class="table table-hover">
      <tr>
        <th>订单号</th>
        <th>物流公司</th>
        <th>运单号</th>
        <th>订单状态</th>
        <th>结果</th>
      </tr>
      <% @results.each do |r| %>
        <tr>
          <td><%= r[:order_number] %></td>
          <td><%= r[:express] %></td>
          <td><%= r[:tracking_number] %></td>
          <td><%= order_status_text(r[:order_status]) %></td>
          <td><%= r[:message] %></td>
        </tr>
      <% end %>
    </table>
  <% end %>
</div>
